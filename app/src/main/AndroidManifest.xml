<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.felipejunges.together">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Controller.EventForm" />
        <activity
            android:name=".Controller.EventRegisterFragments.EventRegister"
            android:label="@string/title_activity_event_register"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Controller.EventRegisterFragments.EventRegisterBaseFragment"
            android:label="@string/title_activity_event_register_base"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Controller.EventRegisterActivity" />
        <activity android:name=".Controller.EventActivity"></activity>
    </application>

</manifest>